        <div class="seperator-hr"></div>

        <div class="margin-bottom">
            <p>
                <strong class="detail-name">
                    <i class="fa fa-building" aria-hidden="true"></i>
                    Loại dự án:
                </strong>
                <strong class="detail-value">@Model.Category.name</strong>
            </p>
            <p>
                <strong class="detail-name">
                    <i class="fa  fa-user"></i>
                    Chủ đầu tư:
                </strong>
                <span class="detail-value">@Model.Investor</span>
            </p>

            <p>
                <strong class="detail-name">
                    <i class="fa  fa-map-marker"></i>
                    Vị trí:
                </strong>
            </p>
            <div class="ebox">
                <p>
                    <i class="fa fa-caret-right" aria-hidden="true"></i>
                    <strong class="detail-name">
                        Thành phố:
                    </strong>
                    <span class="detail-value">@Model.City.name</span>
                </p>
                <p>
                    <i class="fa fa-caret-right" aria-hidden="true"></i>
                    <strong class="detail-name">
                        Quận:
                    </strong>
                    <span class="detail-value">@Model.State</span>
                </p>
                <p>
                    <i class="fa fa-caret-right" aria-hidden="true"></i>
                    <strong class="detail-name">
                        Đường:
                    </strong>
                    <span class="detail-value">@Model.Address</span>
                </p>
            </div>
            <p>
                <strong class="detail-name">
                    <i class="fa  fa-arrows-alt"></i>
                    Quy mô diện tích:
                </strong>
                <span class="detail-value">@Model.Size</span>
            </p>
            <p>
                <strong class="detail-name">
                    <i class="fa fa-home" aria-hidden="true"></i>
                    Tổng số căn hộ:
                </strong>
                <span class="detail-value">@Model.Apartments</span>
            </p>
            <p>
                <strong class="detail-name">
                    <i class="fa  fa-calendar"></i>
                    Năm khởi công:
                </strong>
                <span class="detail-value">@Model.StartYear</span>
            </p>
            <p>
                <strong class="detail-name">
                    <i class="fa  fa-calendar"></i>
                    Dự kiến hoàn thành:
                </strong>
                <span class="detail-value">@Model.CompletedYear</span>
            </p>
            <p>
                <strong>
                    Đơn vị thiết kế:
                </strong>
            </p>

            <div class="ebox">
                <p>
                    <i class="fa fa-caret-right" aria-hidden="true"></i>
                    <strong class="detail-name">
                        Nội thất:
                    </strong>
                    <span class="detail-value">@Model.FurnitureDesignAgency</span>
                </p>
                <p>
                    <i class="fa fa-caret-right" aria-hidden="true"></i>
                    <strong class="detail-name">
                        Kiến trúc:
                    </strong>
                    <span class="detail-value">@Model.ArchitetualDesignAgency</span>
                </p>
                <p>
                    <i class="fa fa-caret-right" aria-hidden="true"></i>
                    <strong class="detail-name">
                        Cảnh quan:
                    </strong>
                    <span class="detail-value">@Model.ViewDesignAgency</span>
                </p>
            </div>
            <p>
                <strong class="detail-name">
                    Đơn vị thi công:
                </strong>
                <span class="detail-value">@Model.ConstructionAgency</span>
            </p>
            <p>
                <strong class="detail-name">
                    Đơn vị quản lý:
                </strong>
                <span class="detail-value">@Model.Manager</span>
            </p>
            <p>
                <strong class="detail-name">
                    Đơn vị phân phối:
                </strong>
                <span class="detail-value">@Model.DistributionAgency</span>
            </p>
        </div>

        <div class="uppercase small-text">Bản đồ</div>
        <div class="seperator-hr"></div>
        <div style='overflow:hidden;height:250px;width:100%;'>
            <div id='gmap_canvas' style='height:250px;width:100%;'></div>
            <style>
                #gmap_canvas img {
                    max-width: none !important;
                    background: none !important;
                }
            </style>
        </div> <a href='https://embedmaps.org/'>embedded google map</a>
    </div>
</div>
<div style="clear:both"></div>

